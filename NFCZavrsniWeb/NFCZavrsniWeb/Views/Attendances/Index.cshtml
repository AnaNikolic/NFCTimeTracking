@model IEnumerable<NFCZavrsniWeb.Models.Entity.Attendance>

@{
    ViewBag.Title = "Index";
}

<h2>All attendances</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee1.OIB)
        </th>
        <th>
            Tag Content
        </th>
        <th>
            Time
        </th>
        <th>
            NFC ContentRead
        </th>
        <th>
            NFC ContentUploaded
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Employee1.OIB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tag1.TagContent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NFCContentRead)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NFCContentUploaded)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID })
        </td>
    </tr>
}

</table>
